# Network Configuration
SOLANA_NETWORK=devnet                                  # mainnet-beta, testnet, devnet, or localnet
SOLANA_RPC_URL=https://api.devnet.solana.com          # Solana RPC endpoint
SOLANA_WS_URL=wss://api.devnet.solana.com             # Solana WebSocket endpoint

# Protocol Configuration
PROGRAM_ID=SYNPSv1protocol11111111111111111111111111111    # Synapse Protocol program ID
PROTOCOL_STATE_ADDRESS=your_protocol_state_address          # Protocol state account address
PROTOCOL_AUTHORITY=your_protocol_authority_address          # Protocol authority address

# Node Configuration
NODE_TYPE=validator                                    # validator, compute, or storage
NODE_STAKE=1000000000                                 # Stake amount in lamports (1 SOL = 1,000,000,000 lamports)
NODE_CAPACITY=medium                                   # low, medium, or high
NODE_KEY_PATH=keys/node.key                           # Path to node keypair file

# API Configuration
PORT=3000                                             # API server port
HOST=0.0.0.0                                          # API server host
API_VERSION=v1                                        # API version
RATE_LIMIT_WINDOW_MS=900000                          # Rate limit window in milliseconds (15 minutes)
RATE_LIMIT_MAX_REQUESTS=100                          # Maximum requests per window
CORS_ORIGIN=*                                        # CORS origin (use specific domain in production)

# Database Configuration
DB_TYPE=redis                                         # redis or leveldb
REDIS_URL=redis://localhost:6379                      # Redis connection URL
LEVELDB_PATH=data/db                                  # LevelDB data directory

# Neural Network Configuration
MODEL_TYPE=prediction                                 # prediction, optimization, or classification
MODEL_PATH=models/default.onnx                       # Path to ONNX model file
INPUT_SHAPE=[1,28,28]                                # Model input shape
OUTPUT_SHAPE=[1,10]                                  # Model output shape
BATCH_SIZE=32                                        # Training batch size
LEARNING_RATE=0.001                                  # Model learning rate

# Security Configuration
JWT_SECRET=your_jwt_secret_key                       # JWT signing secret
ENCRYPTION_KEY=your_encryption_key                   # Data encryption key
MIN_STAKE_THRESHOLD=500000000                        # Minimum stake requirement (0.5 SOL)

# Monitoring Configuration
LOG_LEVEL=debug                                      # debug, info, warn, or error
METRICS_ENABLED=true                                 # Enable metrics collection
METRICS_INTERVAL=60000                               # Metrics collection interval (ms)
ALERT_WEBHOOK_URL=your_webhook_url                   # Alert webhook URL

# Performance Configuration
MAX_CONCURRENT_REQUESTS=100                          # Maximum concurrent requests
CACHE_TTL=300                                        # Cache time-to-live in seconds
WORKER_THREADS=4                                     # Number of worker threads

# Development Configuration
DEBUG=true                                           # Enable debug mode
NODE_ENV=development                                 # development, staging, or production
VERBOSE_LOGGING=true                                 # Enable verbose logging

# WebSocket Configuration
WS_ENABLED=true                                      # Enable WebSocket support
WS_PORT=3001                                         # WebSocket server port
WS_HEARTBEAT_INTERVAL=30000                         # WebSocket heartbeat interval (ms)

# Consensus Configuration
CONSENSUS_THRESHOLD=0.67                             # Required consensus threshold (67%)
PROPOSAL_TIMEOUT=300000                             # Proposal timeout in milliseconds
MIN_VALIDATORS=3                                     # Minimum required validators

# DeFi Configuration
POOL_ADDRESS=your_pool_address                       # Default liquidity pool address
FEE_ACCOUNT=your_fee_account                        # Protocol fee account
BASE_FEE=0.003                                      # Base fee rate (0.3%)

# Testing Configuration
TEST_PRIVATE_KEY=your_test_private_key              # Test wallet private key
MOCK_SERVICES=false                                 # Enable service mocking
TEST_TIMEOUT=5000                                   # Test timeout in milliseconds 